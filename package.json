{
  "name": "@premia/ethers-multicall",
  "version": "1.0.0-beta.2",
  "description": "Make multiple Ethereum network requests in a single HTTP query. ethcall for ethers v5.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha -r @ts-tools/node \"test/**/*.test.ts\"",
    "build": "tsc",
    "lint": "tslint -c tslint.json '{src,test}/**/*.ts' --fix",
    "prepublishOnly": "yarn typechain && yarn build && yarn lint",
    "typechain": "typechain --target ethers-v5 --out-dir ./src/typechain \"./src/abi/*.json\""
  },
  "keywords": [
    "ethers",
    "multicall"
  ],
  "author": "Cavan Flynn <cavan.m.flynn@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@ts-tools/node": "^0.8.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "tslint": "^5.12.0",
    "typechain": "^8.1.1",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "ethers": ">=5 <6"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Premia-Frontend/ethers-multicall.git"
  },
  "bugs": {
    "url": "https://github.com/Premia-Frontend/ethers-multicall/issues"
  },
  "homepage": "https://github.com/cavanmflynn/ethers-multicall#readme"
}
